{
  "name": "backend",
  "version": "0.0.1",
  "description": "<h1 align=\"center\"> Projeto API Synch </h1>",
  "main": "index.js",
  "type": "module",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.5.2",
    "@types/socket.io": "^3.0.1",
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.10",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.6"
  },
  "scripts": {
    "build": "tsc",
    "dev": "concurrently \"tsc --watch\" \"nodemon\"",
    "migrate:dev": "prisma migrate dev --schema ./src/prisma/schema.prisma",
    "generate": "prisma generate --schema ./src/prisma/schema.prisma",
    "studio": "prisma studio --schema ./src/prisma/schema.prisma",
    "reset:db": "prisma migrate reset --schema ./src/prisma/schema.prisma --force",
    "seed": "tsx src/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "prisma": "^6.16.2",
    "socket.io": "^4.8.1"
  },
